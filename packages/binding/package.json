{
	"name": "@contember/binding",
	"license": "Apache-2.0",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/production/index.js",
	"exports": {
		".": {
			"import": {
				"development": "./dist/development/index.js",
				"production": "./dist/production/index.js"
			}
		}
	},
	"files": [
		"dist/"
	],
	"typings": "./dist/binding.d.ts",
	"scripts": {
		"build": "vite build --mode development && vite build --mode production",
		"build:api:dev": "api-extractor run --local",
		"build:api:prod": "api-extractor run",
		"test": "jest",
		"test:with-coverage": "jest --coverage=true"
	},
	"dependencies": {
		"@contember/client": "workspace:*",
		"@contember/react-client": "workspace:*",
		"@contember/react-multipass-rendering": "workspace:*",
		"@contember/react-utils": "workspace:*",
		"chevrotain": "6.5.0",
		"js-levenshtein": "1.1.6",
		"uuid": "8.3.2"
	},
	"peerDependencies": {
		"react": "^17",
		"react-dom": "^17"
	},
	"devDependencies": {
		"@types/jest": "26.0.13",
		"@types/js-levenshtein": "1.1.0",
		"@types/uuid": "8.3.1"
	}
}
