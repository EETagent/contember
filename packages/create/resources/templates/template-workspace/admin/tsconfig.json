{
	"extends": ["../tsconfig.settings.json"],
	"compilerOptions": {
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"lib": [
			"esnext",
			"dom",
			"dom.iterable",
		],
		"paths": {
			"@app/components": [
				"./app/components/*"
			],
			"@app/lib/*": [
				"./lib/*"
			],
			"@app/lib/plugins/*": [
				"./lib/plugins/*"
			]
		}
	}
}
