{
  "scripts-template": {
    "contember": "docker-compose exec admin node_modules/.bin/contember",
    "vite": "docker-compose exec admin node_modules/.bin/vite",
    "tsc": "docker-compose exec admin node_modules/.bin/tsc",
    "start": "npm run start-engine && npm run start-admin",
    "start-engine": "npm run contember migrations:execute . --yes",
    "start-admin": "npm run vite admin --port ${VITE_PORT:-4000} --host 0.0.0.0",
    "build-admin": "npm run tsc && npm run vite build admin"
  },
  "devDependencies": {
    "@contember/cli": "{version}",
    "@contember/schema": "{version}",
    "@contember/schema-definition": "{version}",
    "@contember/admin": "^1.0.0-alpha.6",
    "@types/react": "^17",
    "react": "^17",
    "react-dom": "^17",
    "typescript": "^4.5",
    "vite": "^2.7"
  }
}
