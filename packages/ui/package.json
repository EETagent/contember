{
	"name": "@contember/ui",
	"license": "Apache-2.0",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": [
		"*.sass"
	],
	"main": "./dist/production/index.js",
	"exports": {
		".": {
			"import": {
				"development": "./dist/development/index.js",
				"production": "./dist/production/index.js"
			}
		}
	},
	"typings": "./dist/ui.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build --mode development && vite build --mode production",
		"build:api:dev": "api-extractor run --local",
		"build:api:prod": "api-extractor run",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@blueprintjs/icons": "3.29.0",
		"@contember/react-multipass-rendering": "workspace:*",
		"@mangoweb/sass-base": "0.0.8",
		"@popperjs/core": "2.10.2",
		"classnames": "2.3.1",
		"react-aria": "3.9.0",
		"react-popper": "2.2.5",
		"react-stately": "3.8.0",
		"react-textarea-autosize": "8.3.3"
	},
	"peerDependencies": {
		"react": "^17",
		"react-dom": "^17"
	},
	"devDependencies": {
		"@babel/core": "7.15.5",
		"@storybook/addon-actions": "6.3.8",
		"@storybook/addon-essentials": "6.3.8",
		"@storybook/addon-links": "6.3.8",
		"@storybook/preset-scss": "1.0.3",
		"@storybook/react": "6.3.8",
		"@types/node": "15.0.2",
		"@types/react": "17.0.30",
		"@types/react-dom": "17.0.9",
		"babel-loader": "8.2.2",
		"css-loader": "5.2.4",
		"csstype": "3.0.8",
		"manrope": "2.0.0",
		"resolve-url-loader": "4.0.0",
		"sass": "1.36.0",
		"sass-loader": "10.1.1",
		"source-map-loader": "2.0.1",
		"style-loader": "2.0.0"
	}
}
