{
	"name": "@contember/playground",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "vite build --config vite.config.ts --base /playground/",
		"contember": "docker-compose run contember-cli",
		"deploy": "yarn -- contember deploy playground --admin http://admin-server:4000 --yes",
		"start": "vite --config vite.config.ts --port ${VITE_PORT:-3000} --host 0.0.0.0",
		"test": "vitest --root ./ --environment jsdom"
	},
	"devDependencies": {
		"@contember/brand": "workspace:*",
		"@contember/client-content": "workspace:*",
		"@contember/graphql-builder": "workspace:*",
		"@contember/interface": "workspace:*",
		"@contember/interface-tester": "workspace:*",
		"@contember/react-board": "workspace:*",
		"@contember/react-board-dnd-kit": "workspace:*",
		"@contember/react-dataview": "workspace:*",
		"@contember/react-slots": "workspace:*",
		"graphql": "^16.8.1"
	},
	"dependencies": {
		"@contember/schema": "^1.4.0-beta.1",
		"@contember/schema-definition": "^1.4.0-beta.1",
		"@contember/schema-utils": "^1.4.0-beta.1",
		"@radix-ui/react-alert-dialog": "^1.0.5",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-primitive": "^1.0.3",
		"@radix-ui/react-select": "^2.0.0",
		"@radix-ui/react-slot": "^1.0.2",
		"@radix-ui/react-toast": "^1.1.5",
		"autoprefixer": "^10",
		"class-variance-authority": "^0.7.0",
		"lucide-react": "^0.302.0",
		"postcss": "^8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-twc": "^1.3.0",
		"tailwind-merge": "^2.2.0",
		"tailwindcss": "^3"
	}
}
