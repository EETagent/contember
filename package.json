{
	"type": "module",
	"scripts": {
		"build": "pnpm run ts:build && pnpm run --recursive --parallel build && pnpm run --recursive --parallel build:styles",
		"changeset": "changeset",
		"check": "./scripts/runChecks.sh",
		"clean": "pnpm run clean:dependencies && pnpm run clean:generated",
		"clean:dependencies": "rm -rf node_modules ee/*/node_modules packages/*/node_modules",
		"clean:generated": "rm -rf temp ee/*/dist ee/*/temp packages/*/dist packages/*/temp",
		"eslint:fix": "eslint --fix \"**/*.{ts,tsx}\" ",
		"eslint:lint": "eslint \"**/*.{ts,tsx}\" ",
		"storybook": "cd packages/ui && pnpm run storybook",
		"test": "pnpm run -r --parallel test",
		"ts:build": "tsc --build",
		"ts:watch": "tsc --build --watch",
		"vite": "cd packages/admin-sandbox && pnpm run vite"
	},
	"devDependencies": {
		"@babel/core": "7.14.8",
		"@babel/preset-env": "7.14.8",
		"@babel/preset-react": "7.14.5",
		"@babel/preset-typescript": "7.14.5",
		"@changesets/cli": "2.16.0",
		"@types/classnames": "2.2.10",
		"@types/jest": "26.0.20",
		"@types/react": "17.0.30",
		"@types/react-dom": "17.0.9",
		"@typescript-eslint/eslint-plugin": "4.29.0",
		"@typescript-eslint/parser": "4.29.0",
		"@vitejs/plugin-react-refresh": "1.3.5",
		"babel-jest": "27.0.6",
		"esbuild": "^0.13.15",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"jest": "26.4.2",
		"jsdom": "^17.0.0",
		"nodemon": "2.0.4",
		"prettier": "2.3.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"sass": "1.35.2",
		"tsm": "2.2.1",
		"typescript": "4.3.5",
		"vite": "2.6.14"
	}
}
